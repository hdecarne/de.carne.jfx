<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.carne.jfx.test.JFXTestController">
   <children>
      <MenuBar fx:id="menuBar">
        <menus>
          <Menu fx:id="menuFile" mnemonicParsing="false" text="%_STR_MENU_FILE">
            <items>
              <MenuItem fx:id="menuClose" mnemonicParsing="false" onAction="#onCmdClose" text="%_STR_MENU_CLOSE" />
            </items>
          </Menu>
            <Menu fx:id="menuHelp" mnemonicParsing="false" text="%_STR_MENU_HELP">
               <items>
                  <MenuItem fx:id="menuLogs" mnemonicParsing="false" onAction="#onCmdLogs" text="%_STR_MENU_LOGS" />
                  <MenuItem fx:id="menuAbout" mnemonicParsing="false" onAction="#onCmdAbout" text="%_STR_MENU_ABOUT" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
</VBox>
